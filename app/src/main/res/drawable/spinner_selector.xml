<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_activated="true">

    <shape>
        <solid android:color="@color/selectItemList" />

        <corners

            android:radius="2dp">


        </corners>
    </shape>

    </item>


    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/selectItemList" />

            <corners

                android:radius="2dp">


            </corners>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/selectItemList" />
        </shape>
        <corners

            android:radius="2dp">


        </corners>
    </item>
    <item android:drawable="@color/iconsColor" />




</selector>